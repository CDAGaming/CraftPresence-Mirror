{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "Completely Customize the way others see you play in Discord!",
  "authors": [
    "CDAGaming"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/craftpresence",
    "sources": "https://gitlab.com/CDAGaming/CraftPresence",
    "issues": "https://gitlab.com/CDAGaming/CraftPresence/issues"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.gitlab.cdagaming.craftpresence.fabric.CraftPresenceFabric"
    ],
    "modmenu": [
      "com.gitlab.cdagaming.craftpresence.fabric.impl.ModMenuImpl"
    ]
  },
  "depends": {
    "fabricloader": "${loader_version_range}",
    "minecraft": "${game_version_range}",
    "unilib": ">=${unilib_min_version}",
    "java": ">=17"
